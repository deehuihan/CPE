#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<map>
#include<math.h>
#include<sstream>
using namespace std;

int main() {
	string s1, s2;
	while (true) {
		cin >> s1;
		for (int i = 0; i < s1.length(); i++) {
			s1[i] = tolower(s1[i]);
		}
		cin >> s2;
		for (int i = 0; i < s2.length(); i++) {
			s2[i] = tolower(s2[i]);
		}
		for (int i = s1.length(); i >= 0; i--) {
			for (int j = s2.length(); j >= 0; j--) {
				if (s1[i] == s2[j]) {
					cout << s2[j];
					break;
				}
			}
		}
		cout << endl;
	}
}
