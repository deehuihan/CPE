#include<iostream>
#include<string>
using namespace std;
int main(){
    string s;
    while(getline(cin,s)){
        cout<<endl;
        int ascii[130]={0};
        int max_num=0;
        int count=0;
        for(int i=0;i<s.length();i++){
            ascii[(int)s[i]]++;
            max_num = max(max_num , (int)s[i]);
            count = max(count , ascii[(int)s[i]]);
        }
        for(int i=1;i<=count;i++){
            for(int j=max_num;j>=0;j--){
                if(ascii[j]==i){
                    cout<<j<<" "<<i<<endl;
                }
            }
        }
    }
}
