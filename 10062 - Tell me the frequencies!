#include<iostream>
#include<string>
using namespace std;
int main(){
    string s;
    while(getline(cin,s)){
        cout<<endl;
        int ascii[130]={0};
        int ascii_min = 0;
        int ascii_max=0;
        int ascii_count=0;
        for(int i=0;i<s.length();i++){
            ascii[(int)s[i]]++;
            ascii_min = min(ascii_min , (int)s[i]);
            ascii_max = max(ascii_max , (int)s[i]);
            ascii_count = max(ascii_count , ascii[(int)s[i]]);
        }
        for(int i=1;i<=ascii_count;i++){
            for(int j=ascii_max;j>=0;j--){
                if(ascii[j]==i){
                    cout<<j<<" "<<i<<endl;
                }
            }
        }
    }
}
