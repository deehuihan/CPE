#include<string>
#include<vector>
#include<iomanip>
#include<algorithm>
using namespace std;

int main() {
	char c;
	int len = 0;
	int input[256] = { 0 };
	while (cin >> c) {
		len++;
		input[toupper(c)]++;
	}
	while (len--) {
		if (len == 0) {
			continue;
		}
		for (char c = 'A'; c <= 'Z'; c++) {
			if (len == input[c]) {
				cout << c << " " << len << endl;
			}
		}
	}
}
