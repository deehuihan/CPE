#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<map>
#include<math.h>
#include<sstream>
using namespace std;

int main() {
	int len = 0;
	int input[256] = { 0 };
	char c;
	while (cin >> c) {
		len++;
		input[toupper(c)]++;
	}
	while (--len) {
		for (c = 'A'; c <= 'Z'; c++) {
			if (input[c] == len) {
				cout << c << " " << input[c] << endl;
			}
		}
	}
}
