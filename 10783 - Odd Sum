#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<map>
#include<math.h>
#include<sstream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while (t--) {
		int k = 1;
		int a, b;
		cin >> a >> b;
		int answer = 0;
		for (int i = a; i <= b; i++) {
			if (i % 2 == 1) {
				answer += i;
			}
		}
		cout << "Case " << k << ": " << answer << endl;
		k++;
	}
}
