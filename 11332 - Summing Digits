#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<map>
#include<math.h>
#include<sstream>
using namespace std;

int main() {
	unsigned int n;
	while (cin >> n) {
		if (n == 0) {
			break;
		}
		while (n / 10 != 0) {
			n = n / 10 + n % 10;
		}
		cout << n << endl;
	}
}
