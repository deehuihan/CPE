
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <iomanip>
using namespace std;

int main()
{
	int t;
	string word;
	cin >> t;
	getline(cin, word);
	getline(cin, word);
	while (t--) {
		vector<string>vec;
		vec.clear();
		while (getline(cin, word) && word != "") {
			vec.push_back(word);
		}
		sort(vec.begin(), vec.end());
		int count = 1;
		double sum = 0, p;
		sum = vec.size();
		for (int i = 0; i < sum; i++) {
			if (vec[i] == vec[i + 1]) {
				count++;
			}
			else if(vec[i]!=vec[i+1]){
				p = (count / sum) * 100;
				cout << fixed << setprecision(4);
				cout << vec[i] << " " << p << endl;
				count = 1;
			}
		}
		if (t != 0) {
			cout << endl;
		}
	}
}
