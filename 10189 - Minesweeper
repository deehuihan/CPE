#include<iostream>
using namespace std;
int main() {
	int n, m, count = 1;
	char word;
	while (cin >> n >> m) {
		if (n == 0 && m == 0) {
			break;
		}
		getchar();
		int field[100][100] = { 0 };
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= m; j++) {
				word = getchar();
				if (word == '*') {
					field[i][j] = -100;
					for (int k = -1; k <= 1; k++) {
						for (int l = -1; l <= 1; l++) {
							field[i - k][j - l]++;
						}
					}
				}
			}
			getchar();
		}
		cout << "Field #" << count << ":" << endl;
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= m; j++) {
				if (field[i][j] < 0) {
					cout << "*";
				}
				else {
					cout << field[i][j];
				}
			}
			cout << endl;
		}
	}
}
